    "    .dropna()\n",
    "    .query(\"GEO != ['Nunavut', 'Northwest Territories']\")\n",
    "    #drop more rows\n",
    "    .rename(columns={\"REF_DATE\":\"Year\", \"GEO\":\"Location\", \"SCALAR_FACTOR\":\"Scalar Factor\", \"VALUE\":\"Value\"})\n",
    "    .assign(weightedAvg= lambda x: x.Value) \n",
    ")\n",
